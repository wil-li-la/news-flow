{
  "name": "newsflow-mobile",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "ios": "expo start --ios",
    "android": "expo start --android",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "api": "npm --prefix .. run dev:api",
    "start:with-api": "../node_modules/.bin/concurrently \"npm --prefix .. run dev:api\" \"expo start\"",
    "ios:with-api": "../node_modules/.bin/concurrently \"npm --prefix .. run dev:api\" \"expo start --ios\"",
    "android:with-api": "../node_modules/.bin/concurrently \"npm --prefix .. run dev:api\" \"expo start --android\""
  },
  "dependencies": {
    "@aws-amplify/react-native": "^1.1.10",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "aws-amplify": "^6.15.7",
    "expo": "~51.0.0",
    "expo-linking": "~6.3.0",
    "expo-router": "^3.5.16",
    "expo-status-bar": "~1.12.1",
    "lucide-react-native": "^0.378.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "^15.2.0"
  },
  "devDependencies": {
    "@types/react": "~18.2.79",
    "@types/react-native": "0.73.0",
    "babel-preset-expo": "^11.0.6",
    "typescript": "5.3.3"
  }
